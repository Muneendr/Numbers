class Main {
    public static void main(String[] args) {
       int binary = 1011;
       double sum=0;
       int i=0;
       while(binary>0)
       {
           int temp = binary%10;
           sum=sum+(Math.pow(2,i))*temp;
           binary=binary/10;
           i++;
       }
       System.out.println((int)sum);
    }
}
