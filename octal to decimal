class Main {
    public static void main(String[] args) {
       int octal=345;
      int decimal=0;
       int power=0;
      while(octal>0)
      {
          int lastdigit = octal%10;
          decimal+=lastdigit*Math.pow(8,power);
          octal=octal/10;
          power++;
      }
      System.out.println(decimal);
    }
}
