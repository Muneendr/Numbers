class Main {
    public static void main(String[] args) {
      int number=18;
      int sum=0;
      for(int i=1;i*i<number;i++)
      {
          if(number%i==0)
          {
              System.out.println(i);
              sum=sum+i;
          
          if(i!=number/i)
          {
              if(number/i!=number)
              {
                    sum+=number/i;
              System.out.println(number/i);
              }
        
          }
          }
      }
      System.out.println("The sum of the divisors of the number is "+sum);
      if(sum>number)
      {
          System.out.println("The given number is a abundant number");
      }
      else
      {
          System.out.println("The given number is not a abundant number");
      }
    }
}
